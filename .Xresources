!Xft.dpi:       128
!Xft.rgba:      rgba
!Xft.antialias: 1
!Xft.hinting:   1
!Xft.autohint:  0
!Xft.hintstyle: hintslight

URxvt*font: xft:Source Code Pro:regular:size=12:antialias=true:hinting=true:autohint=false:hintstyle=hintnone:dpi=96:minspace=False, xft:Roboto Condensed:pixelsize=18
URxvt*boldFont: xft:Source Code Pro:bold:size=12:antialias=true:hinting=true:autohint=false:hintstyle=hintnone:dpi=96:minspace=False, xft:Roboto Condensed:pixelsize=18
URxvt*italicFont: xft:Source Code Pro:italic:size=12:antialias=true:hinting=true:autohint=false:hintstyle=hintnone:dpi=96:minspace=False, xft:Roboto Condensed:pixelsize=18
URxvt*boldItalicfont: xft:Source Code Pro:italic:size=12:antialias=tre:hinting=true:autohint=false:hintstyle=hintnone:dpi=96, xft:Roboto Condensed:pixelsize=18

!:antialias=false:hintstyle=hintfull


!! URxvt Appearance

URxvt*letterSpace: 0
URxvt.geometry: 92x24
URxvt.imLocale: en_US.UTF-8
URxvt.lineSpace: 0
URxvt.loginShell: true

URxvt.depth: 32
URxvt.background: [75]#2b303b

URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true


